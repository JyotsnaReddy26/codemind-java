import java.util.Scanner;
class Average
{
  public static void main(String args[])
{
  Scanner sc=new Scanner(System.in);
    double a,b,c,d,e;
      Double Average;
     a=sc.nextDouble();
     b=sc.nextDouble();
     c=sc.nextDouble();
     d=sc.nextDouble();
     e=sc.nextDouble();
     Average=(a+b+c+d+e)/5;
System.out.println("Average of numbers is:"+Average);
}
}
