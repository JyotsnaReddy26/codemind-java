import java.util.Scanner;
public class Mono
{
  public static void main(String args[])
  {
    Scanner sc=new Scanner(System.in);
int T;
T=sc.nextInt();
  while(T-->0){
    int r1,r2,r3;
    r1=sc.nextInt();
    r2=sc.nextInt();
    r3=sc.nextInt();
    if(r1>(r2+r3)||r2>(r3+r1)||r3>(r2+r1))
    {
       System.out.println("YES");
    }
    else
    {
        System.out.println("NO");
    }
  }
    
  }
}
