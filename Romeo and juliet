import java.util.Scanner;
public class RJ
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x,y,z;
        x=sc.nextInt();
        y=sc.nextInt();
        z=sc.nextInt();
        int k;
        k=(5*x+10*y)/z;
        System.out.println(k);
            }
}
